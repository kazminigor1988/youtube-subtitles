{
  "presets": [
    "env"
  ],
  "plugins": [
    [
      "transform-runtime",
      {
        "polyfill":    false,
        "regenerator": true
      }
    ],
    [
      "module-resolver",
      {
        "root":  [
          "./"
        ],
        "alias": {
          "js": "./js"
        }
      }
    ]
  ]
}
